Submitted Code

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int n,a[100];
int x=0;

    scanf("%d ",&n);
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(int i=0;i<n;i++)
    {
         x=x+a[i];
       
    }
    printf("Array Mean Value is %.2f",(float)x/n);
    

}
