
Factorial Number
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n,x=1;
    scanf("%d",&n);
    if(n>0){
    for(int i=1;i<=n;i++){
        x*=i;
    }
    printf("The Factorial of %d is %d",n,x);
        }
    else
        printf("Invalid Input");
    return 0;
}
