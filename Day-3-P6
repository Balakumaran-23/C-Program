Search an Element

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int a[100],x,n,m=0;
    scanf("%d",&n);
    for(int i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    scanf("%d",&x);
    for(int i=0;i<n;i++)
    {
        if(a[i]==x)
           m=1;
    }
    if(m==1)
        printf("%d is presented in an array.",x);
    else
        printf("%d is not presented in an array.",x);
    return 0;
}
