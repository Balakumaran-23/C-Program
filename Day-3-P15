Best Leader

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n;
    scanf("%d",&n);
    int a[n],c[n];
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    int b=a[n-1];
    printf("The Best Leaders are ");
    for(int i=n-1;i>=0;i--){
        if(b<=a[i]){
            c[i]=a[i];
            b=a[i];
            printf("%d ",c[i]);
            
        }
    }
    
    return 0;
}
