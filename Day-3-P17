Sparse Matrix

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n;
    scanf("%d",&n);
    int a[n][n],cnt1=0,cnt2=0;
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            scanf("%d",&a[i][j]);
        }
    }
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            if(a[i][j]==0)
                cnt1++;
            else
                cnt2++;
        }
    }
    if(cnt1<cnt2)
        printf("Matrix is not a Sparse Matrix");
    else
        printf("Matrix is a Sparse Matrix");
    return 0;
}
