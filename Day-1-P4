Eligible for Placement

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char a[100];
    int i,j;
    scanf("%s\n%d\n%d",a,&i,&j);
    printf("Name of the Student:%s\n",a);
    if(i==1&&j>=70)
        printf("%s is Eligible for Placement",a);
    else if((i==1||i==2) && j>=75)
        printf("%s is Eligible for Placement",a);
    else
        printf("%s is Not Eligible for Placement",a);
    return 0;
}
