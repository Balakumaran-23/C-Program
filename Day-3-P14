Maximum Product Of Two Elements In An Array 1

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n;
    scanf("%d", &n);
    int a[n];
    
    for(int i = 0; i < n; i++) {
        scanf("%d", &a[i]);
    }

    int max_product = 0;

    for(int i = 0; i < n - 1; i++) {
        for(int j = i + 1; j < n; j++) {
            int product = (a[i] - 1) * (a[j] - 1);
            if(product > max_product) {
                max_product = product;
            }
        }
    }

    printf("Maximum product of two elements in an array is %d", max_product);
    return 0;
}
